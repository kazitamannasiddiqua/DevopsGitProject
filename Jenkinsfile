    sh 'echo "Compile Project"'
    sh 'sleep 5'

        
    }
    stage('Testing'){
        sh 'echo "Testing Project"'
        sh 'sleep 5'
 
    }
    stage('Integration'){
        sh 'echo "Interation out"'
        sh 'sleep 5'
    }
    stage('Deployment'){
        sh 'echo "Deployment Project"'
        sh 'sleep 5'
    }



}